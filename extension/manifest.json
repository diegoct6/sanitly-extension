{
  "manifest_version": 3,
  "name": "Sanitly",
  "version": "0.1",
  "permissions": ["sidePanel", "activeTab", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "side_panel": {
    "default_path": "panel.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
